{
    "name": "nikrou/phyxo",
    "description": "Simply share your photos",
    "homepage": "http://www.phyxo.net",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Nicolas Roudaire",
            "email": "nikrou77@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "ircmaxell/password-compat": "^1.0",
        "natxet/CssMin": "^3.0",
        "openpsa/universalfeedcreator": "^1.8",
        "pclzip/pclzip": "^2.8",
        "pelago/emogrifier": "^2.0",
        "smarty/smarty": "^3.1",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/routing": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/security-csrf": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "tedivm/jshrink": "^1.3"
    },
    "require-dev": {
        "atoum/atoum": "^3.0",
        "atoum/reports-extension": "^3.0",
        "behat/behat": "^3.0",
        "behat/common-contexts": "^1.2",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behatch/contexts": "^3.0",
        "composer/xdebug-handler": "^1.2",
        "dmore/behat-chrome-extension": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.14",
        "jdorn/sql-formatter": "^1.2",
        "symfony/dom-crawler": "^3.3",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Phyxo\\": "src/Phyxo/",
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
